{
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/.*",
        "dest": "/index.html",
        "status": 200
      }
    ],
    "rewrites": [
      { "source": "/post/:id", "destination": "/index.html" },
      { "source": "/create", "destination": "/index.html" },
      { "source": "/edit/:id", "destination": "/index.html" },
      { "source": "/login", "destination": "/index.html" },
      { "source": "/register", "destination": "/index.html" }
    ]
  }
  